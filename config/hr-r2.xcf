<APPLICATION Parent="defaultwa">
  <RESOURCE Id="res.path.fglprofile" Source="ENVIRON">FGLPROFILE</RESOURCE>
  <EXECUTION>
    <ENVIRONMENT_VARIABLE Id="DBDATE">DMY4/</ENVIRONMENT_VARIABLE>
    <ENVIRONMENT_VARIABLE Id="FGLPROFILE">db.prof:$(res.path.fglprofile)</ENVIRONMENT_VARIABLE>
    <PATH>$(res.deployment.path)/.</PATH>
    <MODULE>Emp_Main</MODULE>
    <PARAMETERS>
      <!-- Responsive mode -->
      <PARAMETER>-R2</PARAMETER>
    </PARAMETERS>
  </EXECUTION>
</APPLICATION>
